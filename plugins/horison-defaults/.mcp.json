{
  "supabase": {
    "type": "http",
    "url": "https://mcp.supabase.com/mcp"
  },
  "neo4j": {
    "command": "uvx",
    "args": [
      "--with",
      "fastmcp<3",
      "mcp-neo4j-cypher@0.5.2",
      "--transport",
      "stdio"
    ],
    "env": {
      "NEO4J_URI": "${NEO4J_URI}",
      "NEO4J_USERNAME": "${NEO4J_USERNAME}",
      "NEO4J_PASSWORD": "${NEO4J_PASSWORD}",
      "NEO4J_DATABASE": "${NEO4J_DATABASE}"
    }
  },
  "langfuse": {
    "type": "http",
    "url": "https://langfuse.horison.ai/api/public/mcp",
    "headers": {
      "Authorization": "Basic ${LANGFUSE_MCP_AUTH}"
    }
  },
  "langfuse-docs": {
    "type": "http",
    "url": "https://langfuse.com/api/mcp"
  },
  "serena": {
    "command": "uvx",
    "args": [
      "--from",
      "git+https://github.com/oraios/serena",
      "serena-mcp-server",
      "--context",
      "claude-code"
    ]
  },
  "context7": {
    "command": "npx",
    "args": [
      "-y",
      "@upstash/context7-mcp@latest"
    ]
  },
  "playwright": {
    "command": "npx",
    "args": [
      "-y",
      "@playwright/mcp@latest"
    ]
  },
  "memory": {
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-memory"
    ]
  }
}
