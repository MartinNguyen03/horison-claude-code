{
  "supabase": {
    "type": "http",
    "url": "https://mcp.supabase.com/mcp"
  },
  "serena": {
    "command": "uvx",
    "args": [
      "--from",
      "git+https://github.com/oraios/serena",
      "serena-mcp-server",
      "--context",
      "claude-code"
    ]
  },
  "context7": {
    "command": "npx",
    "args": [
      "-y",
      "@upstash/context7-mcp@latest"
    ]
  },
  "playwright": {
    "command": "npx",
    "args": [
      "-y",
      "@playwright/mcp@latest"
    ]
  },
  "memory": {
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-memory"
    ]
  },
  "firebase": {
    "command": "npx",
    "args": [
      "-y",
      "firebase-tools@latest",
      "mcp"
    ]
  },
  "langfuse-docs": {
    "transportType": "http",
    "url": "https://langfuse.com/api/mcp",
    "verifySsl": true
  }
}
